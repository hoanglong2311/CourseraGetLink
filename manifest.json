{
    "manifest_version": 2,

    "name": "Coursera Getlink",
    "description": "This extension help you to get your submission link.",
    "version": "1.0",

    "browser_action": {
        "default_icon": "img/icon.png",
        "default_title": "Test",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "<all_urls>"
    ]
}